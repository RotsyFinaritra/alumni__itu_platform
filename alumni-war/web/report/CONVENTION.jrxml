				<textElement>
					<font isBold="true" isUnderline="true"/>
				</textElement>
				<text><![CDATA[D- DE L'OBLIGATION DES PARTIES:]]></text>
			</staticText>
			<textField pattern="">
				<reportElement x="0" y="559" width="515" height="15" uuid="59f2d2ff-39f1-44ae-9823-992396495725"/>
				<textElement textAlignment="Justified"/>
				<textFieldExpression><![CDATA["Article 4: L\'ATT a pour obligation de:"]]></textFieldExpression>
			</textField>
			<line>
				<reportElement x="0" y="570" width="40" height="1" uuid="717487c2-7f52-4c7d-9411-5e71de12ef29"/>
